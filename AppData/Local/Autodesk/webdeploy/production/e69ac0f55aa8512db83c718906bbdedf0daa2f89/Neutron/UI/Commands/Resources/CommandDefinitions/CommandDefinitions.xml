  <CommandDefinition Id="A360RenderCommand">
    <Tooltip>
      <ExpandedContent>
        <TextBlock _LCLZId="lclzA360RenderCommand1" _LCLZText="Creates a high-quality rendered image of your scene using cloud or local background rendering technology."/>
        <TextBlock/>
        <TextBlock _LCLZId="lclzA360RenderCommand2" _LCLZText="Choose the desired image size, settings, cloud or local render, and click Render to start the rendering process. Finished renderings will appear in your Rendering Gallery."/>
      </ExpandedContent>
    </Tooltip>
    <TipsAndTricks MoreInfo="RENDER-IN-THE-CLOUD-CMD">
      <Title _LCLZId="lclA360RenderTipsAndTricksTitle" _LCLZText="Rendering Information"/>
      <Tips _LCLZId="lclzA360RenderTipsAndTricksTips" _LCLZText="Select from one of the presets or configure custom settings. Click Render to start the rendering process. A thumbnail is displayed in the Rendering Gallery."/>
      <Description _LCLZId="lclzA360RenderTipsAndTricksDesc" _LCLZText="Larger, or higher-quality images will require Cloud Credits."/>
    </TipsAndTricks>
  </CommandDefinition>
  <CommandDefinition Id="SaveAsImageCommand">
    <Tooltip>
      <TextBlock _LCLZId="lclzSaveAsImageCommandText" _LCLZText="Capture Image"/>
      <ExpandedContent>
         <TextBlock _LCLZId="lclzSaveAsImageCommand1" _LCLZText="Captures the current canvas as an image."/>
         <TextBlock/>
         <TextBlock _LCLZId="lclzSaveAsImageCommand2" _LCLZText="Set the image size and resolution then set the file type (*.png, *.jpg, *.tiff) and file name."/>
      </ExpandedContent>      
    </Tooltip>
    <TipsAndTricks MoreInfo="RENDER-CAPTURE-CURRENT-IMAGE-CMD">
      <Title _LCLZId="lclzSaveAsImageTipsAndTricksTitle" _LCLZText="Capture Image Information"/>
      <Tips _LCLZId="lclzSaveAsImageTipsAndTricksTips" _LCLZText="Captures the current canvas as an image. Set the image size and resolution then set the file type (*.png, *.jpg, *. tiff) and filename."/>
      <Description _LCLZId="lclzSaveAsImageTipsAndTricksDesc" _LCLZText="For best results, set the environment and camera settings and enable In-canvas Render before capturing the screen."/>
    </TipsAndTricks>
  </CommandDefinition>
